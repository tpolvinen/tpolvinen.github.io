---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Tatu Polvinen's thesis blog</title>
	<link href="https://tpolvinen.github.io/atom.xml" rel="self"/>
	<link href="https://tpolvinen.github.io"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://tpolvinen.github.io</id>
	<author>
		<name>Tatu Polvinen</name>
		<email>tatu.polvinen@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://tpolvinen.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://tpolvinen.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>